{
  "name": "@l0vest0rm/gulp-uglify",
  "description": "Minify files with UglifyJS. forked from https://github.com/terinjokes/gulp-uglify/",
  "version": "1.0.2",
  "author": "Terin Stock <terinjokes@gmail.com>",
  "bugs": {
    "url": "https://github.com/terinjokes/gulp-uglify/issues"
  },
  "dependencies": {
    "array-each": "^1.0.1",
    "extend-shallow": "^3.0.2",
    "gulplog": "^2.0.1",
    "has-gulplog": "^1.0.0",
    "isobject": "^4.0.0",
    "make-error-cause": "^1.1.1",
    "safe-buffer": "^5.2.1",
    "through2": "^4.0.2",
    "uglify-js": "^3.17.4"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^50.0.1",
    "power-assert": "^1.6.1",
    "prettier": "^3.2.5",
    "source-list-map": "^2.0.1",
    "tape": "^5.7.4",
    "tape-catch": "^1.0.6",
    "testdouble": "^3.20.1",
    "vinyl": "^3.0.0"
  },
  "homepage": "https://github.com/l0vest0rm/nodelib",
  "keywords": [
    "gulpplugin"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/l0vest0rm/nodelib.git"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "xo",
      "prettier"
    ],
    "plugins": [
      "unicorn",
      "no-use-extend-native",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "printWidth": 80,
          "tabWidth": 2,
          "singleQuote": true,
          "trailingComma": "none",
          "bracketSpacing": false
        }
      ]
    }
  },
  "files": [
    "index.js",
    "composer.js",
    "lib/"
  ],
  "scripts": {
    "lint": "eslint *.js lib test",
    "test": "tape test/*.js",
    "publish": "npm publish --access public"
  },
  "greenkeeper": {
    "ignore": [
      "gulp-sourcemaps"
    ]
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
